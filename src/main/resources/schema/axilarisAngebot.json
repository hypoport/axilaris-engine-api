    "type": "object",
    "properties": {
      "produktAnbieter": {
        "type": "string"
      },
      "produktBezeichnung": {
        "type": "string"
      },
      "angebotsVariantenTyp": {
        "type": "string"
      },
      "kreditKonditionen": {
        "type": "object",
        "properties": {
          "effektivzinssatz": {
            "$ref": "axilarisDefinitions.json#/percent"
          },
          "sollzinssatz": {
            "$ref": "axilarisDefinitions.json#/percent"
          },
          "gesamtbetrag": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "nettokreditbetrag": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "auszahlungsbetrag": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "laufzeitInMonaten": {
            "type": "integer"
          },
          "rateProMonat": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "letzteRate": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "provision": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "basisbetragFuerDieProvisionsermittlung": {
            "$ref": "axilarisDefinitions.json#/EUR"

{
  "angebot": {
    "type": "object",
    "properties": {
      "produktanbieter": {
        "type": "string"
      },
      "produktbezeichnung": {
        "type": "string"
      },
      "angebotsVariantenTyp": {
        "type": "string"
      },
      "kreditkonditionen": {
        "type": "object",
        "properties": {
          "effektivzinssatz": {
            "$ref": "axilarisDefinitions.json#/percent"
          },
          "sollzinssatz": {
            "$ref": "axilarisDefinitions.json#/percent"
          },
          "gesamtbetrag": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "nettokreditbetrag": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "auszahlungsbetrag": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "laufzeitInMonaten": {
            "type": "integer"
          },
          "rateProMonat": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "letzteRate": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "provision": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "basisbetragFuerDieProvisionsermittlung": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "europaceNutzungsgebuehr": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "produktanbieterZahltEuropaceNutzungsgebuehr": {
            "type": "boolean"
          },
          "vorlaufzinsenProTag": {
            "$ref": "axilarisDefinitions.json#/EUR"
          }
        },
        "additionalProperties": false
      },
      "versicherungskonditionen": {
        "type": "object",
        "properties": {
          "basisFuerDieProvisionsermittlung": {
            "enum": [
              "VERSICHERUNGS_PRAEMIE_NETTO",
              "VERSICHERUNGS_PRAEMIE_BRUTTO"
            ]
          },
          "versicherungsprovision": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "versicherungspraemieBrutto": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "versicherungspraemieNetto": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "versicherungsrisiken": {
            "$ref": "axilarisDefinitions.json#/versicherungsrisiken"
          },
          "versicherer": {
            "type": "string"
          },
          "europaceNutzungsgebuehr": {
            "$ref": "axilarisDefinitions.json#/EUR"
          }
        },
        "additionalProperties": false
      },
      "status": {
        "type": "object",
        "properties": {
          "machbarkeitsstatus": {
            "enum": [
              "MACHBAR",
              "MACHBAR_UNTER_VORBEHALT_PRODUKTANBIETER",
              "NICHT_MACHBAR"
            ]
          },
          "angepasst": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "meldungen": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "kategorie": {
              "enum": [
                "MACHBARKEIT",
                "VOLLSTAENDIGKEIT",
                "HINWEIS",
                "ANPASSUNG"
              ]
            },
            "text": {
              "type": "string"
            },
            "code": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "erforderlicheUnterlagen": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "bonitaetsCheck": {
        "type": "object",
        "properties": {
          "einnahmen": {
            "$ref": "#/bonitaetsCheckPosition"
          },
          "ausgaben": {
            "$ref": "#/bonitaetsCheckPosition"
          },
          "ueberschuss": {
            "$ref": "axilarisDefinitions.json#/EUR"
          }
        },
        "additionalProperties": false
      },
      "maximalerAuszahlungsbetrag": {
        "$ref": "axilarisDefinitions.json#/EUR"
      }
    },
    "additionalProperties": false
  },
  "bonitaetsCheckPosition": {
    "type": "object",
    "properties": {
      "positionen": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "label": {
              "type": "string"
            },
            "wert": {
              "$ref": "axilarisDefinitions.json#/EUR"
            }
          },
          "additionalProperties": false
        }
      },
      "summe": {
        "$ref": "axilarisDefinitions.json#/EUR"
      }
    },
    "additionalProperties": false
  }
}
        "additionalProperties": false
      },
      "uebersichten": {
        "type": "object",
        "properties": {
          "bausteinUebersicht": {
            "$ref": "axilarisDefinitions.json#/uebersicht"
          },
          "boniCheck": {
            "$ref": "axilarisDefinitions.json#/uebersicht"
          }
        },
        "additionalProperties": false
      },
      "maximalerAuszahlungsbetrag": {
        "$ref": "axilarisDefinitions.json#/EUR"
      }
    },
    "additionalProperties": false

            "$ref": "axilarisDefinitions.json#/EUR"
          }
        },
        "additionalProperties": false
      },
      "versicherungsKonditionen": {
        "type": "object",
        "properties": {
          "basisFuerDieProvisionsErmittlung": {
            "enum": [
              "VERSICHERUNGS_PRAEMIE_NETTO",
              "VERSICHERUNGS_PRAEMIE_BRUTTO"
            ]
          },
          "versicherungsProvision": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "versicherungsPraemieBrutto": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "versicherungsPraemieNetto": {
            "$ref": "axilarisDefinitions.json#/EUR"
          },
          "versicherungsrisiken": {
            "$ref": "axilarisDefinitions.json#/versicherungsrisiken"
          },
          "versicherer": {
            "type": "string"
          },
          "europaceNutzungsgebuehr": {
            "$ref": "axilarisDefinitions.json#/EUR"
          }
        },
        "additionalProperties": false
      },
      "status": {
        "type": "object",
        "properties": {
          "machbarkeitsStatus": {
            "enum": [
              "MACHBAR",
              "MACHBAR_UNTER_VORBEHALT_PRODUZENT",
              "NICHT_MACHBAR"
            ]
          },
          "angepasst": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "meldungen": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "kategorie": {
              "enum": [
                "MACHBARKEIT",
                "VOLLSTAENDIGKEIT",
                "HINWEIS",
                "ANPASSUNG"
              ]
            },
            "text": {
              "type": "string"
            },
            "code": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "erforderlicheUnterlagen": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "alternativesAngebot": {
        "type": "object",
        "properties": {
          "alternativesAngebot": {
            "type": "boolean"
          },
          "angebotsAlternativenTyp": {
